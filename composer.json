{
    "name": "Mezek/showmi",
    "type": "typo3-cms-extension",
    "description": "Show database listings",
    "authors": [],
    "require": {
        "typo3/cms-core": "^8.7.1"
    },
    "autoload": {
        "psr-4": {
            "Mezek\\Showmi\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mezek\\Showmi\\Tests\\": "Tests"
        }
    },
    "replace": {
        "showmi": "self.version",
        "typo3-ter/showmi": "self.version"
    }
}